{
  "name": "DvhbApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "storybook": "IS_STORYBOOK=true npm start",
    "storybook:server": "start-storybook",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "tslint -p tsconfig.json -c tslint.json -t verbose --fix",
    "tsc": "tsc",
    "generate:icons": "svgr --native --ext tsx --no-svgo -d src/ui/Icon/components src/ui/Icon/src",
    "generate:index": "create-index src/ui/Icon/components -x tsx",
    "prestorybook": "rnstl",
    "messages": "messages extract",
    "messages:clean": "messages clean"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "intl": "^1.2.5",
    "react": "16.8.6",
    "react-intl": "^3.2.1",
    "react-native": "0.60.5",
    "react-native-config": "git+https://github.com/luggit/react-native-config.git",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "^1.4.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-reanimated": "^1.2.0",
    "react-native-svg": "^9.7.1",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/runtime": "7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@dvhb/react-intl-messages": "^2.4.2",
    "@dvhb/tslint-config-react": "^1.0.9",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-ondevice-actions": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react-native": "^5.1.11",
    "@storybook/react-native-server": "^5.2.1",
    "@svgr/cli": "^4.3.2",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "@types/react-native": "0.60.7",
    "@types/react-navigation": "^3.0.8",
    "@types/react-test-renderer": "16.9.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.2.1",
    "create-index": "^2.4.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "metro-react-native-babel-preset": "0.54.1",
    "prettier": "^1.18.2",
    "react-dom": "^16.8.6",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.8.6",
    "tslint": "^5.19.0",
    "typescript": "3.5.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./storybook"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "messages": {
    "messagesDir": "src/messages",
    "langs": "ru",
    "pattern": "src/**/*.{ts,tsx}"
  }
}
